{
    "dotnet.defaultSolution": "./DS4WindowsWPF.sln",
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": false,
    "dotnet.enableXamlTools": true,
    "dotnet.server.useOmnisharp": true,
    "csharp.semanticHighlighting.enabled": true,
    "csharp.inlayHints.enableInlayHintsForParameters": true,
    "csharp.inlayHints.enableInlayHintsForLiteralParameters": true,
    "csharp.inlayHints.enableInlayHintsForIndexerParameters": true,
    "csharp.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "csharp.inlayHints.enableInlayHintsForOtherParameters": true,
    "csharp.inlayHints.enableInlayHints": true,
    "csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
    "csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
    "omnisharp.enableRoslynAnalyzers": false,
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableAsyncCompletion": true,
    "omnisharp.path": "latest",
    "omnisharp.useGlobalMono": "never",
    "omnisharp.waitForDebugger": false,
    "omnisharp.loggingLevel": "error",
    "omnisharp.maxProjectResults": 250,
    "omnisharp.projectLoadTimeout": 60,
    "omnisharp.enableMsBuildLoadProjectsOnDemand": false,
    "csharp.suppressDotnetInstallWarning": true,
    "csharp.maxProjectFileCountForDiagnosticAnalysis": 1000,
    "csharp.showOmnisharpLogOnError": false,
    "csharp.suppressDotnetRestoreNotification": true,
    "problems.showCurrentInStatus": false,
    "problems.sortOrder": "severity",
    "problems.decorations.enabled": false,
    "csharp.format.enable": false,
    "csharp.codeLens.enableReferencesCodeLens": false,
    "files.exclude": {
        "**/bin": false,
        "**/obj": true,
        "**/.vs": true
    },
    "search.exclude": {
        "**/bin": false,
        "**/obj": true,
        "**/.vs": true
    }
}